{
  "name": "ecofront",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "@mui/styled-engine-sc": "^5.6.1",
    "env-cmd": "^10.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "start:dev": "env-cmd -f .env.dev react-scripts start",
